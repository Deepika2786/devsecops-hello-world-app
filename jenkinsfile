pipeline {
    agent any 

    stages {
        stage('Checkout') {
            steps {
                git url: 'https://github.com/Deepika2786/devsecops-hello-world-app.git', branch: 'master', credentialsId: 'github-cred' 
            }
        }

        stage('Hello World') {
            steps {
                echo 'Hello from Jenkins Pipeline! Code checked out successfully.'
            }
        }
    }
}